# Multi-Agent Orchestrator Configuration
# Copy this file to .env and add your API keys

# ============================================================
# API Keys (at least ONE required)
# ============================================================

# OpenAI (GPT-4, GPT-4o, GPT-4o-mini)
# Get yours: https://platform.openai.com/api-keys
OPENAI_API_KEY=

# Anthropic Claude (Claude 3.5 Sonnet)
# Get yours: https://console.anthropic.com/settings/keys
ANTHROPIC_API_KEY=

# Google Gemini (Gemini 1.5 Pro, Flash)
# Get yours: https://aistudio.google.com/app/apikey
GOOGLE_API_KEY=

# OpenRouter (optional - access to 100+ models)
# Get yours: https://openrouter.ai/keys
OPENROUTER_API_KEY=

# ============================================================
# Testing & Development
# ============================================================

# Enable mock LLM responses (for testing without API keys)
LLM_MOCK=false

# Server port
PORT=5050

# ============================================================
# Provider Feature Flags (optional)
# ============================================================

# Uncomment to disable specific providers
# DISABLE_OPENAI=1
# DISABLE_ANTHROPIC=1
# DISABLE_GOOGLE=1
# DISABLE_OPENROUTER=1
